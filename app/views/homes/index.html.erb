        <!-- Intro Header -->
<header class="intro">
    <div class="intro-body">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <h1 class="brand-heading">Portfolio</h1>
                    <p class="intro-text">Photographic Works<br>By Joel Davison</p>
                    <a href="#download" class="btn btn-circle page-scroll">
                        <i class="fa fa-angle-double-down animated"></i>
                    </a>
                   <!--  if params=photo.id then -->
                   <!--  link_to"Buy a print of this photo" /photographs/id=params > -->
                   <p> <%= link_to 'Buy a print of this photo', photograph_path(@photo) %> </p>
                </div>
            </div>
        </div>
    </div>
</header>

          <!-- Styles section -->

          <section id="download" class ="container content-section text-center">
            <div class="row">
              <% @photographs.each do |photo| %>
                <div class="col-md-4">
                  <div class="card card-inverse">
                    <a href="<%= photograph_path(@photo.id) %>">
                    <img class="card-img" src="<%= photo.image %>" width="100%" height="210px" alt="photograph.catagory">
                    </a>
                    <div class="card-img-overlay">
                      <h4 class"card-title"> <%= photo.title %> </h4>
                    </div>
                  </div>
                  </div>
                <% end %>
            </div>


