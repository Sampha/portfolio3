<%= simple_form_for(@photograph) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :image %>
    <%= f.input :descritpion %>
    <%= f.input :active %>
    <%= f.input :is_cover %>
    <%= f.input :order %>
    <%= f.input :price_small %>
    <%= f.input :price %>
    <%= f.input :price_large %>
    <%= f.input :price_xlarge %>
    <%= f.association :photo_catagory %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
